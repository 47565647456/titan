{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },

  "Parameters": {
    "postgres-password": "TestPassword123ABC"
  },
  "Database": {
    "Pool": {
      "MaxPoolSize": 100,
      "MinPoolSize": 0,
      "ConnectionLifetimeSeconds": 300,
      "ConnectionIdleLifetimeSeconds": 300
    },
    "Server": {
      "MaxConnections": 500,
      "SharedBuffersMB": 256
    }
  },
  "Orleans": {
    "Replicas": 2
  },
  "ContainerImages": {
    "Redis": {
      "Image": "redis",
      "Tag": "8.4"
    },
    "Postgres": {
      "Image": "postgres",
      "Tag": "17"
    },
    "OpenSSL": {
      "Image": "alpine/openssl",
      "Tag": "3.5.4"
    }
  }
}
